package control

alias hienoa (h.command) {
    if (!h.command) {
	xecho -b Usage: /hienoa <unload>
	return
    }
    switch ($h.command) {
	(reload) { xecho -b This feature is no longer available }
	(unload) { h.unload }
    }
}

alias h.unload {
    ^dump all
    timer -d all
    xecho -say $G hienoa unloaded
}

